.top {
  height: 50px;
  display: flex;
  justify-content: space-between;
  align-items: center;
  margin: 0 10px;
}
.top .ipt {
  width: 220px;
  height: 30px;
  border-radius: 15px;
  background-color: #f3f4f4;
  font-size: 14px;
  padding-left: 20px;
}
.top .van-icon {
  font-size: 20px;
}
.title {
  padding: 5px 15px;
  display: flex;
  justify-content: space-between;
}
.title ._span {
  color: #999;
}
.list {
  width: 100%;
  height: 100px;
  display: flex;
  flex-wrap: wrap;
  margin-left: 10px;
}
.list ._span {
  margin: 5px 10px;
  display: inline-block;
  height: 15px;
  border-radius: 5px;
  border: 1px solid #666;
  font-size: 13px;
  padding: 5px;
  color: #999;
  text-align: center;
  line-height: 15px;
}
.Ulll {
  width: 100%;
  display: flex;
  flex-wrap: wrap;
  margin-left: 10px;
}
.Ulll ._li {
  margin: 5px 10px;
  display: inline-block;
  height: 15px;
  border-radius: 5px;
  border: 1px solid #666;
  font-size: 13px;
  padding: 5px;
  color: #999;
  text-align: center;
  line-height: 15px;
}

