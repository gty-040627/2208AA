{
	"pages": [ //pages数组中第一项表示应用启动页，参考：https://uniapp.dcloud.io/collocation/pages
		{
			"path": "pages/index/index",
			"style": {
				"navigationBarTitleText": "在线教育"
			}
		}, {
			"path": "pages/home/home",
			"style": {
				"navigationBarTitleText": "",
				"enablePullDownRefresh": false
			}

		}, {
			"path": "pages/read/read",
			"style": {
				"navigationBarTitleText": "",
				"enablePullDownRefresh": false,
				"onReachBottomDistance": 150,
				"usingComponents": {
					"van-tab": "/wxcomponents/dist/tab/index",
					"van-tabs": "/wxcomponents/dist/tabs/index"
				}

			}

		}, {
			"path": "pages/answer/answer",
			"style": {
				"navigationBarTitleText": "",
				"enablePullDownRefresh": false
			}

		}, {
			"path": "pages/my/my",
			"style": {
				"navigationBarTitleText": "",
				"enablePullDownRefresh": false
			}
		}, {
			"path": "pages/search/search",
			"style": {
				"navigationBarTitleText": "",
				"enablePullDownRefresh": false,
				"usingComponents": {
					"van-icon": "/wxcomponents/dist/icon/index",
					"van-search": "/wxcomponents/dist/search/index"
				}
			}

		}, {
			"path": "pages/search2/search2",
			"style": {
				"navigationBarTitleText": "",
				"enablePullDownRefresh": false,
				"usingComponents": {
					"van-icon": "/wxcomponents/dist/icon/index",
					"van-search": "/wxcomponents/dist/search/index",
					"van-tab": "/wxcomponents/dist/tab/index",
					"van-tabs": "/wxcomponents/dist/tabs/index",
					"van-dropdown-menu": "/wxcomponents/dist/dropdown-menu/index",
					"van-dropdown-item": "/wxcomponents/dist/dropdown-item/index"
				}
			}

		}
	],

	"globalStyle": {
		"navigationBarTextStyle": "black",
		"navigationBarTitleText": "在线教育",
		"navigationBarBackgroundColor": "#F8F8F8",
		"backgroundColor": "#F8F8F8"

	},
	"tabBar": {
		"color": "#000000",
		"selectedColor": "#345dc2",
		"borderStyle": "black",
		"list": [{
				"pagePath": "pages/index/index",
				"text": "首页",
				"iconPath": "/static/tabber-icon/index.png",
				"selectedIconPath": "/static/tabber-icon/index-active.png"
			},
			{
				"pagePath": "pages/home/home",
				"text": "分类",
				"iconPath": "/static/tabber-icon/home.png",
				"selectedIconPath": "/static/tabber-icon/home-active.png"
			},
			{
				"pagePath": "pages/read/read",
				"text": "阅读",
				"iconPath": "/static/tabber-icon/read.png",
				"selectedIconPath": "/static/tabber-icon/read-active.png"
			},
			{
				"pagePath": "pages/answer/answer",
				"text": "问答",
				"iconPath": "/static/tabber-icon/question.png",
				"selectedIconPath": "/static/tabber-icon/question-active.png"
			},
			{
				"pagePath": "pages/my/my",
				"text": "我的",
				"iconPath": "/static/tabber-icon/my.png",
				"selectedIconPath": "/static/tabber-icon/my-active.png"
			}
		]
	},
	"uniIdRouter": {}
}