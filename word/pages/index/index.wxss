.big{
  background-color: #cccccc31;
}
.navber{
  white-space: nowrap;
  width: 100%;
  height: 80rpx;
  background-color: #fff;
  /* margin-bottom: 20rpx; */
}
.navber-item{
  padding: 0 20rpx;
}
swiper{
  margin-top: 40rpx;
  width: 100%;
  height: 400rpx;
}
swiper swiper-item{
   width: 100%;
   height: 100%;
   margin: 0 auto;
} 
swiper swiper-item image{
  border-radius: 30rpx;
  width: 100%;
  height: 100%;
}
.lll{
  width: 90%;
  height: 100%;
  background-color: #fff;
  margin: 0 auto;
  border-radius: 60rpx;
}
.lll image{
  width: 200rpx;
  height: 70rpx;
  line-height: 80rpx;
  padding-left: 50rpx;
}
.lll text{
  /* padding-bottom: 20rpx; */
  font-size: 35rpx;
  /* padding-top: -50rpx; */
}
.yyy{
  width: 100%;
  height: 150rpx;
  line-height: 150rpx;
  text-align: center;
  color: rgb(136, 132, 132);
}
.list{
  width: 95%;
  border-radius: 20rpx;
  height: 700rpx;
  background-color: #fff;
  margin: 40rpx auto;
}
.list .top{
  width: 180rpx;
  color: #fff;
  height: 50rpx;
  background-color: #f15a24;
  border-radius: 20rpx 0 20rpx 0;
  text-align: center;
}
.title{
  padding: 20rpx 50rpx;
}
.tt{
  display: flex;
  flex-wrap: wrap;
}
.getteacher{
  width: 30%;
  margin: 20rpx auto;
  display: flex;
}

.getteacher image{
  width: 70rpx;
  height: 70rpx;
  border-radius: 50%;
}
.man{
  width: 100%;
  height: 100rpx;
  padding-left: 40rpx;
  font-size: 32rpx;
  color: rgb(116, 111, 111);
}
.price{
  display: flex;
  justify-content: space-between;
}
.price :nth-child(1){
  color: rgb(110, 107, 107);
  font-size: 32rpx;
  padding-left: 30rpx;
}
.price .ppp{
  color: red;
  font-size: 40rpx;
  padding-left: 30rpx;
}