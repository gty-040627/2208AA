/**index.wxss**/
page{
  background-color: #f6f0e3;
}
.list{
  display: flex;
  justify-content: space-around;
  flex-wrap: wrap;
}
.address{
  width: 100vw;
}
.address .boxx{
  width: 100%;
  height: 70rpx;
  line-height: 70rpx;
  border-bottom: 2rpx solid #ccc;
  display: flex;
  justify-content: space-between;
  font-size: 35rpx;
}
.box{
  width: 100%;
  height: 70rpx;
  line-height: 70rpx;
  border-bottom: 2rpx solid #ccc;
  display: flex;
  justify-content: space-between;
  padding-left: 20rpx;
  color: rgb(148, 143, 143);
}
.van-icon{
  padding: 0 10rpx;
}
.ccc{
  color: rgb(153, 147, 147);
}

.title{
  width: 100vw;
  height: 140rpx;
  line-height: 140rpx;
  text-align: center;
  font-size: 45rpx;
}